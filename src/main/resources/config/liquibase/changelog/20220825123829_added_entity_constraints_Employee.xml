<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Employee.
    -->
    <changeSet id="20220825123829-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="affectation_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee__affectation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="affectation"/>

        <addForeignKeyConstraint baseColumnNames="etablissement_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee__etablissement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="etablissement"/>

        <addForeignKeyConstraint baseColumnNames="affectation_admin_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee__affectation_admin_id"
                                 referencedColumnNames="id"
                                 referencedTableName="affectation_admin"/>
    </changeSet>
</databaseChangeLog>
