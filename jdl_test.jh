


// an ignored comment
/** not an ignored comment */

/**
 * Task entity.
 * @author The JHipster team.
 */
entity Affectation {
	nomprojet String,
	description String,
    datedebut Instant,
    datefin Instant
}
entity Etablissement{
	nometablissement String,
    manager Long,
    teamlead Long
}

/**
 * The Employee entity.
 */
entity Employee {
	/**
	* The firstname attribute.
	*/
	nom String,
	prenom String,
	email String,
	numtel String,
	dateembauche Instant
}

entity Projet {
	titreprojet String
}

entity Entretien {
	dateentretient Instant,
    resultat String,
    confirmationresponsable String,
    confirmationsalarie String
}
entity AffectationAdmin{
     datedebut Instant,
     datefin Instant
}
entity Competance {
	nomCompetance String,
    description String,
    niveau Integer
}
relationship ManyToOne {
	Employee to Affectation,
    Competance to Affectation
    }
relationship OneToMany {
    Employee to Entretien
    Projet to Affectation

}

relationship ManyToOne {
     Employee to Etablissement,
     Employee to Etablissement,
     Etablissement to AffectationAdmin,
     Employee to AffectationAdmin
}

// defining multiple OneToMany relationships with comments



// defining multiple oneToOne relationships


// Set pagination options
paginate Affectation, Employee with infinite-scroll

// Use Data Transfer Objects (DTO)
// dto * with mapstruct

// Set service options to all except few
service all with serviceImpl except Employee, Affectation

