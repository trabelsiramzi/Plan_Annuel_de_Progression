


// an ignored comment
/** not an ignored comment */

/**
 * Task entity.
 * @author The JHipster team.
 */
entity Affectation {
	nomprojet String,
	description String,
    datedebut Instant,
    datefin Instant
}

/**
 * The Employee entity.
 */
entity Employee {
	/**
	* The firstname attribute.
	*/
	nom String,
	prenom String,
	email String,
	numtel String,
	dateembauche Instant
}

entity Projet {
	titreprojet String,
}

entity Entretien {
	dateentretient Instant,
    resultat String,
    confirmationresponsable String,
    confirmationsalarie String,
}

entity Responsable {
	 nommanager String,
     nomteamlead String
}

relationship OneToMany {
	Employee{nomprojet} to Affectation,
    Responsable to Entretien
}

relationship OneToOne {
	Projet{nomprojet} to Affectation
}

relationship ManyToOne {
     Employee{Manager} to Employee
}
relationship OneToOne {
	Department{location} to Location
}

// defining multiple OneToMany relationships with comments



// defining multiple oneToOne relationships


// Set pagination options
paginate Affectation, Employee with infinite-scroll

// Use Data Transfer Objects (DTO)
// dto * with mapstruct

// Set service options to all except few
service all with serviceImpl except Employee, Affectation

